class Person
  #include(Object)
  include(BasePerson)
#self == Person
  merge(self, {
    hello: \self -> puts("hello every one!");,
    init: \self -> puts("i'm initialized"),
    say: \self ->
      if super.say
        super.say(self)
        #super == BasePerson
      else
        puts("nothing")
      ;
  })
;

